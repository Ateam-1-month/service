<%= form_with url: "/companies", model: @company, local: true do |f| %>
    <%= render 'shared/error_messages', obj: @company %>
    <%= f.label :name, "会社名" %>
    <%= f.text_field :name %>
    <%= f.label :image, "画像(あとで変更する)" %>
    <%= f.text_field :image %>
    <%= f.submit "Create my account"%>
<% end %>